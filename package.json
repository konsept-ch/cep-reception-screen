{
  "name": "cep-reception-screen",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.11.0"
  },
  "scripts": {
    "start": "cross-env TSC_COMPILE_ON_ERROR=true DISABLE_ESLINT_PLUGIN=true PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint:check": "eslint --ext .js,.jsx,.ts,.tsx . --report-unused-disable-directives",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "stylelint:check": "stylelint **/*.scss",
    "stylelint:fix": "stylelint --ext .js,.jsx,.ts,.tsx . --fix",
    "prettier:check": "prettier . --ignore-unknown --check",
    "prettier:fix": "prettier . --ignore-unknown --write",
    "test-related": "cross-env NODE_ENV=test CI=true npm run test -- --bail --findRelatedTests",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@fontsource/roboto": "4.5.8",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.10.0",
    "@reduxjs/toolkit": "1.8.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.2",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "imagemin-lint-staged": "0.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-scripts": "5.0.1",
    "sass": "1.54.3",
    "stylelint": "14.9.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-csstree-validator": "2.0.0",
    "typescript": "4.7.4",
    "web-vitals": "2.1.4"
  }
}
